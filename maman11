Object subclass: #Book
    instanceVariableNames: 'title author year pages'
    classVariableNames: ''
    poolDictionaries: ''
    category: 'Library'


!Book methodsFor: 'initialization'!

initializeWithTitle: aTitle author: anAuthor year: aYear pages aPages
    title := aTitle.
    author := anAuthor.
    year := aYear.
    pages := aPages.
!

